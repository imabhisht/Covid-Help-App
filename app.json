{
  "expo": {
    "name": "Covid-Help",
    "slug": "CovidHelpApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mainIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/mainSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abhisht.covidhelp"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mainIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abhisht.CovidHelpApp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
